SET(CMAKE_INCLUDE_CURRENT_DIR ON)
INCLUDE_DIRECTORIES(${WLINK_SOURCE_DIR}/kal/include)
INCLUDE_DIRECTORIES(${WLINK_SOURCE_DIR}/hal/include)
INCLUDE_DIRECTORIES(${WLINK_SOURCE_DIR}/hal/src)
INCLUDE_DIRECTORIES(${WLINK_SOURCE_DIR}/rac/include)
INCLUDE_DIRECTORIES(${WLINK_SOURCE_DIR}/at/include)
LINK_DIRECTORIES (${WLINK_SOURCE_DIR}/rac/lib)

ADD_EXECUTABLE(sensor_master sensor_master.c)
TARGET_LINK_LIBRARIES(sensor_master KAL_LIB HAL_LIB RAC_LIB AT_LIB)

